    "autoprefixer": "^9",
    "classnames": "^2.3.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "gh-pages": "^2.2",
    "husky": "^4.2.1",
    "keymirror": "^0.1.1",
    "lint-staged": "^10.0.7",
    "postcss": "^7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^8.0.1",
